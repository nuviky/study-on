<form method="post" action="{{ path('course_delete', {'id': course.id}) }}" onsubmit="return confirm('Вы уверены что хотите удалить данную программу обучения?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ course.id) }}">
    <button class="btn btn-secondary course-delete" id="course-delete">Удалить курс</button>
</form>
